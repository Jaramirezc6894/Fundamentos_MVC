@model IEnumerable<Fundamentos_MVC.Models.Restaurant>

@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1>ASP.NET</h1>
    <div>
        There are @Enumerable.Count(Model) number of restaurants in the db.</div>
    <div>
        There are @Model.Count() restaurants int the db.
    </div>
    <ul>
        @foreach(var restaurant in Model)
        {
            <li>@restaurant.Id</li>
            <li>@restaurant.Name</li>
        }
    </ul>
</div>